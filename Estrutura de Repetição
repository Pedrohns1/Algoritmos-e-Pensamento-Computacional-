//Exemplo while controle automatico
#include <stdio.h>

int main()
{
    int num = 1;
    while (num <=10){
        printf ("%i\n", num);
        num = num+1;
    }

    return 0;
}



//Exemplo do-while controle automatico
#include <stdio.h>

int main()
{
    int num = 0, acum = 0;
    do{
        printf ("%i\n", num);
        acum = acum+acum;
        num = num+1;
    }while (num <= 10);
    printf ("Soma: %i", acum);
    return 0;
}



//Exemplo for
#include <stdio.h>

int main()
{
    int num, acum =0;
    for (num = 0; num<=10; num++){
        printf ("%i\n", num);
        acum = acum+num;
    }
    printf ("Soma: %i", acum);
    return 0;
}
